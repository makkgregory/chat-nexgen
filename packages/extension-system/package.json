    "eslint": "^9.33.0",
    "@eslint/js": "^9.33.0",
    "typescript-eslint": "^8.39.1",
    "globals": "^16.3.0",
    "vitest": "^3.2.4",
    "@vitest/ui": "^3.2.4"
